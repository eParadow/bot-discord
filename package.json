    "pg": "^8.13.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "zod": "^3.22.4",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
