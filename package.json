{
  "name": "bot-discord",
  "version": "1.0.0",
  "description": "Bot Discord avec syst√®me de rappels CRON",
  "main": "dist/index.js",
  "scripts": {
    "dev:bot": "tsx watch src/bot/index.ts",
    "dev:server": "tsx watch src/server/index.ts",
    "dev": "npm run dev:bot & npm run dev:server",
    "build:bot": "tsc --project tsconfig.bot.json",
